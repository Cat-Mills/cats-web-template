    "autoprefixer": "^10.4.16",
    "aws-sdk": "^2.1472.0",
    "axios": "^1.5.1",
    "bcryptjs": "^2.4.3",
    "circular-json": "^0.5.9",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "morgan": "^1.10.0",
    "nodemon": "^3.0.1",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "postcss": "^8.4.31",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-plock": "^3.0.2",
    "react-router-dom": "^6.16.0",
    "react-toggle-dark-mode": "^1.1.1",
    "sequelize": "^6.33.0",
    "stripe": "^13.9.0",
